  "main": "lib/index.js",
  "keywords": [
    "Oracle Coherence",
    "Coherence",
    "gRPC",
    "node"
  ],
  "repository": "https://github.com/oracle/coherence-js-client",
  "dependencies": {
    "grpc": "^1.24.3",
    "@grpc/proto-loader": "^0.5.4",
    "google-protobuf": "^3.12.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.2",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^4.1.0",
    "mocha": "^8.0.1",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "postinstall": "bin/npm-post-install.sh",
